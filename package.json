{
  "name": "scs-syntax-highlighting",
  "displayName": "schedule script",
  "description": "schedule script syntax highlighting",
  "author": {
    "name": "insberr",
    "email": "main@insberr.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/insberr/scs-syntax-highlighting.git"
  },
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "scs",
      "aliases": ["schedule script", "scs"],
      "extensions": [".scs"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "scs",
      "scopeName": "source.scs",
      "path": "./syntaxes/scs.tmLanguage.json"
    }]
  }
}
